@inherits JssComponentBase

@RenderRawComponent()

@code {
    private RenderFragment RenderRawComponent() => builder =>
    {
        builder.OpenElement(0, Component.Name);
        builder.AddMultipleAttributes(1, Component.Attributes.ToDictionary(kvp => kvp.Key, kvp => (object)kvp.Value));
        builder.AddContent(2, Component.Contents);
        builder.CloseElement();
    };
}
