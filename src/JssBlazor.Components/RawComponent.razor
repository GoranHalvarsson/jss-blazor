@inherits JssComponentBase

@RenderRawComponent()

@code {
    private RenderFragment RenderRawComponent() => builder =>
    {
        builder.OpenElement(0, Component.Name);
        foreach (var attribute in Component.Params)
        {
            builder.AddAttribute(0, attribute.Key, attribute.Value);
        }
        builder.AddContent(0, Component.Contents);
        builder.CloseElement();
    };
}
